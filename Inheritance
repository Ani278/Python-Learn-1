class Animal():
    def __init__(self):
        print('ANIMAL CREATED')
    def who_am_i(self):
        print("I am an animal")
    def eat(self):
        print("i am eating")
myanimal =Animal()
myanimal.eat()
myanimal.who_am_i()



class Dog(Animal):
    def __init__(self):
        Animal.__init__(self)
        print("Dog created!")
    def who_am_i(self):
        print("i am a dog")
    def bark(self):
        print("woof!")
   
mydog=Dog()
mydog.who_am_i()
mydog.bark()
