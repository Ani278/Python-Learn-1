x =25
def printer():
    x= 50
    return x

print(x)

print(printer())
name ='THIS IS A GLOBAL STRING'
def greet():
    name='Sammy'

def hello():
    print('Hello'+name)

hello()

greet()
x=50

def func(x):
    print(f'X is {x}')

x=200
print(f'I JUST LOCALLY CHANGED X TO 200')

func(x)
print(x)

x=50
def func(x):
    global x
    print(f'X is {x}')
    x='NEW VALUE'
    print(f'I JUST LOCALLY CHANGED X TO {x}')

func(x)
print(x)
